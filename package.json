{
  "name": "verdaccio-htpasswd",
  "version": "1.0.1",
  "description": "verdaccio htpasswd auth plugin",
  "main": "lib/index.js",
  "keywords": [
    "verdaccio",
    "local-storage",
    "plugin"
  ],
  "author": "Ayush Sharma <ayush.aceit@gmail.com>",
  "private": false,
  "license": "MIT",
  "scripts": {
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "coverage": "codecov",
    "release": "standard-version -a -s",
    "test": "jest",
    "lint": "npm run flow && npm run js-lint",
    "js-lint": "eslint .",
    "flow": "flow",
    "build": "babel src/ --out-dir lib/ --copy-files --ignore ___tests___",
    "precommit": "lint-staged",
    "prepublish": "in-publish && npm run lint && npm run build || not-in-publish"
  },
  "dependencies": {
    "@verdaccio/file-locking": "^0.0.7",
    "apache-md5": "^1.1.2",
    "bcryptjs": "2.4.3",
    "unix-crypt-td-js": "^1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "6.1.3",
    "@commitlint/config-conventional": "6.1.3",
    "@verdaccio/types": "3.7.2",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-jest": "23.4.0",
    "babel-plugin-flow-runtime": "0.17.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-flow": "6.23.0",
    "codecov": "3.0.4",
    "cross-env": "5.2.0",
    "eslint": "5.6.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "2.6.2",
    "flow-bin": "0.81.0",
    "flow-runtime": "0.17.0",
    "husky": "^0.14.3",
    "in-publish": "^2.0.0",
    "jest": "23.6.0",
    "lint-staged": "7.2.0",
    "prettier": "1.13.7",
    "standard-version": "4.4.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=6.12.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js, json}": [
      "eslint .",
      "prettier --write",
      "git add"
    ]
  }
}
